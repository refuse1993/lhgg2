generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = "postgres://postgres.fqbrhecrnqwtnqrlysxx:votmdnjemanjffhgo123@aws-0-ap-southeast-1.pooler.supabase.com:6543/postgres?pgbouncer=true"
}

model Posts {
  id        Int      @id @default(autoincrement())
  userId    String   @db.Uuid
  name      String
  image     String
  text      String
  picture   String
  createdAt DateTime @default(now())
  likes     Likes[]
}

model Likes {
  id        Int      @id @default(autoincrement())
  userId    String   @db.Uuid
  postId    Int
  createdAt DateTime @default(now())
  post      Posts    @relation(fields: [postId], references: [id], onDelete: Cascade)
}
